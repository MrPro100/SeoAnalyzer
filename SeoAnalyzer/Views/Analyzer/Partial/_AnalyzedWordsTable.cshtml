@model IEnumerable<SeoAnalyzer.Models.AnalyzedWordForTable>


<link href="~/Content/DataTables-1.10.2/css/dataTables.bootstrap.css" rel="stylesheet" />

<script src="~/Scripts/DataTables-1.10.2/jquery.dataTables.min.js"></script>

<script src="~/Scripts/DataTables-1.10.2/dataTables.bootstrap.js"></script>

<script>
    $(document).ready(function ()
    {
        $('#@ViewData["TableId"].ToString()-table').dataTable({
            "order": [[1, "desc"]],
            "lengthChange": false,
            "stateSave": true,
        });
    })
</script>

<div class="panel panel-default">
    <div class="panel-body">

        <table id="@ViewData["TableId"].ToString()-table" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Word</th>
                    <th>Occurrences</th>
                    <th>Density, %</th>
                </tr>
            </thead>

            <tbody>
                @foreach (SeoAnalyzer.Models.AnalyzedWordForTable row in Model)
                {
                    <tr>
                        <td><p>@row.Word</p></td>
                        <td><p>@row.WordOccurrencesCount</p></td>
                        <td><p>@row.PercentWordCount</p></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>