@model SeoAnalyzer.Models.Error

<div id="error" class="alert alert-danger alert-dismissible" role="alert">
    <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
    <h2><span class="glyphicon glyphicon-exclamation-sign"></span> Server Error</h2>
    <div class="row">
        <div class="col-xs-12 col-md-12">
            <strong>Message: </strong> @Model.ErrorException.Message 
        </div>
        <div class="col-xs-12 col-md-12">
            <strong>Error Stack Trace: </strong> @Model.ErrorException.StackTrace
        </div>
    </div>
</div>